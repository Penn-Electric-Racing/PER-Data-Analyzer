# Penn Electric Racing MCP Server Configuration
# Copy this file to .env and fill in your values

# S3 Configuration for fetching GeneratedCanIds.xml
S3_BUCKET=your-bucket-name
S3_KEY=your-access-key
S3_SECRET=your-secret-key
S3_ENDPOINT=https://your-endpoint.com

# Optional: Local path to GeneratedCanIds.xml (if not using S3)
#CAN_DEFINES_PATH=/path/to/GeneratedCanIds.xml

# Optional: Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
